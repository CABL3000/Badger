$('#blast_fig').empty();
var drawing = new BioDrawing();
drawing.start(800, 'blast_fig');
drawing.drawSpacer(50);
drawing.drawTitle('Testing biodrawing');
drawing.drawScale(600);
var myBar = drawing.drawBar(100, 400, 15, 'blue', '', '');
myBar.click(function(){ document.url('www.google.com');});

drawing.end();
